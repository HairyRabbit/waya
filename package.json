{
  "name": "waya",
  "version": "0.0.3",
  "description": "Yet another builder",
  "main": "index.js",
  "bin": "bin/waya",
  "files": [
    "project/**/*",
    "**/*.js",
    "**/*.json",
    "**/*.d.ts",
    "favicon.ico",
    "logo.svg"
  ],
  "scripts": {
    "start": "tsc -w",
    "test": "jest",
    "build": "tsc",
    "build:bin": "bin-file-gen -y",
    "build:logo": "cp logo.svg dist",
    "buildico": "node -e \"require('favicons')('./logo.svg',{icons:{favicons:true,android:false,appleIcon:false,appleStartup:false,coast:false,firefox:false,windows:false,yandex:false}}).then(d=>d.images.find(v=>v.name==='favicon.ico').contents).then(c=>fs.writeFileSync('./dist/favicon.ico',c,'utf-8')).then(_=>process.exit(0)).catch(_=>process.exit(2))\"",
    "build:project": "cp project -r dist",
    "postbuild": "npm-run-all -p build:**"
  },
